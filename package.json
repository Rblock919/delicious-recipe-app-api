{
  "name": "recipe-application",
  "version": "1.0.1",
  "description": "Application for storing and displaying delicious recipes.",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "start.dev": "nodemon",
    "start.all.dev": "concurrently \"nodemon\" \"cd ../recipe-app-gql && nodemon\" \"cd ../recipe-app-ng && ng serve\"",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "format": "prettier --write 'src/**/*.ts'",
    "lint": "eslint --fix 'src/**/*.ts'",
    "lint.probe": "eslint --fix-dry-run 'src/**/*.ts'",
    "preinstall": "rimraf node_modules",
    "postinstall": "npm run build"
  },
  "author": "Ryan Block",
  "license": "ISC",
  "devDependencies": {
    "@rblock919/eslint-config": "^1.1.2",
    "@types/express": "^4.17.3",
    "@types/jest": "^25.1.4",
    "@types/mongoose": "^5.7.6",
    "@types/node": "^13.9.1",
    "@types/node-fetch": "^2.5.5",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "concurrently": "^5.1.0",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3",
    "wiredep": "^4.0.0"
  },
  "dependencies": {
    "bcrypt": "^3.0.8",
    "body-parser": "^1.19.0",
    "chalk": "^3.0.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "express-session": "^1.17.0",
    "express-validator": "^6.4.0",
    "graphql": "^14.6.0",
    "helmet": "^3.21.3",
    "jsonwebtoken": "^9.0.0",
    "mongodb": "^3.5.5",
    "mongoose": "^5.9.4",
    "rimraf": "^3.0.2"
  },
  "engines": {
    "node": "12.11.0",
    "npm": "6.13.7"
  }
}
